<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="spRootFranchiseListPane" stylesheets="@../css/TranxCommonCssStyles/tranxCommonCssStyle7.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.opethic.genivis.controller.master.FranchiseListController">

    <top>
      <VBox id="topMainDiv">
         <children>
              <HBox alignment="CENTER_LEFT" spacing="10.0">
                  <TextField id="tfSearchField" fx:id="tffieldSearch" promptText="Search" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Button fx:id="btnFranchiseListCreate" styleClass="submit-btn" text="Create" />
              </HBox>
         </children>
      </VBox>
    </top>

    <center>
        <TableView id="tranxListTableViewStyle" fx:id="tvfranchiseList" editable="true">
            <columns>
                <TableColumn fx:id="tcfranchiseListId" text="Sr.No." />
                <TableColumn fx:id="tcfranchiseListName" text="Franchise Name" />
                <TableColumn fx:id="tcfranchiseListCode" text="Franchise Code" />
                <TableColumn fx:id="tcfranchiseListAddress" text="Address" />
                <TableColumn fx:id="tcfranchiseListMobileNumber" text="Mobile Number" />
                <TableColumn fx:id="tcfranchiseListPincode" text="Pincode" />

            </columns>
        </TableView>
    </center>

</BorderPane>
