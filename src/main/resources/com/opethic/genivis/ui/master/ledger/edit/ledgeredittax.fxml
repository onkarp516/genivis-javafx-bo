<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="stpaneother" style="-fx-background-color: #ffffff;" stylesheets="@../../../css/ledger_css_style/ledger_css_7.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.opethic.genivis.controller.master.ledger.edit.LedgerEditTaxController">
    <top>
        <VBox id="ledgerTopMainDiv">
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
            <children>
                <VBox id="innerDivSd">
                    <HBox alignment="CENTER_LEFT" spacing="10.0">
                        <Label text="Tax Type">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </Label>
                        <ComboBox fx:id="cmbLedgerEditTax">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </ComboBox>
                        <padding>
                            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                        </padding>
                    </HBox>
                </VBox>
            </children>
        </VBox>
    </top>
    <bottom>
        <HBox id="sqbuttonsMainDiv" alignment="CENTER_RIGHT" spacing="10.0" BorderPane.alignment="CENTER">
            <Button fx:id="btnLedgerEditTaxSubmit" mnemonicParsing="false" styleClass="submit-btn" text="Update" />
            <Button fx:id="btnLedgerEditTaxCancel" mnemonicParsing="false" styleClass="cancel-btn" text="Cancel">
                <HBox.margin>
                    <Insets right="12.0" />
                </HBox.margin>
            </Button>
        </HBox>
    </bottom>
</BorderPane>
